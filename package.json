{
  "name": "vue-meow-ui",
  "version": "1.0.3",
  "private": false,
  "description": "个人练手组件库",
  "author": "Justin <nbilly@163.com>",
  "homepage": "https://github.com/MrTenger/meow-ui.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrTenger/meow-ui.git"
  },
  "bugs": {
    "url": "https://github.com/MrTenger/meow-ui/issues"
  },
  "keywords": [
    "meow-ui",
    "vue",
    "component"
  ],
  "files": [
    "lib",
    "packages"
  ],
  "license": "MIT",
  "main": "lib/meow-ui.umd.min.js",
  "style": "lib/meow-css/index.css",
  "scripts": {
    "bootstrap": "npm install && cd ./packages/_style && npm install",
    "dev": "npm run build:entry && vue-cli-service serve",
    "dev:docs": "vuepress dev docs",
    "build:example": "vue-cli-service build",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "lint": "vue-cli-service lint && eslint --fix --ext .js,.vue packages examples",
    "test:unit": "vue-cli-service test:unit",
    "lib": "vue-cli-service build --target lib --name meow-ui  --dest lib packages/index.js && webpack  --config ./build/webpack.component.js",
    "build:entry": "node build/build-entry.js ",
    "build:lib": "node build/build-lib.js ",
    "build:style": "gulp build --gulpfile packages/_style/gulpfile.js && rimraf lib/meow-css && cp-cli packages/_style/lib lib/meow-css",
    "build:docs": "vuepress build docs"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-mocha": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "chai": "^4.1.2",
    "core-js": "^2.6.5",
    "chalk": "^4.0.0",
    "cp-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs-extra": "^8.0.1",
    "node-sass": "^4.9.0",
    "postcss-pxtorem": "^5.1.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.4",
    "signale": "^1.4.0",
    "svg-sprite-loader": "^4.1.6",
    "uppercamelcase": "^3.0.0",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^0.14.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 0.01%",
    "Android >= 4.0",
    "Firefox ESR",
    "iOS >= 8.0",
    "not ie <= 8"
  ]
}
