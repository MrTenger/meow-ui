{
  "name": "vue-meow-ui",
  "version": "1.0.0",
  "private": false,
  "description": "喵呜～个人练手组件库",
  "author": "Justin <nbilly@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrTenger/meow-ui.git"
  },
  "main": "lib/meow-ui.umd.min.js",
  "style": "lib/_style/meow-ui.css",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "clean": "rimraf lib && rimraf packages/*/lib",
    "lib": "npm run clean && vue-cli-service build --target lib --name meow-ui --dest lib packages/index.js && npm run build:style",
    "build:style": "gulp build --gulpfile packages/_style/gulpfile.js && rimraf lib/meow-css && cp-cli packages/_style/lib lib/meow-css",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "cp-cli": "^2.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuepress": "^0.14.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-mocha": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "browserslist": [
    "> 0.01%",
    "Android >= 4.0",
    "Firefox ESR",
    "Ios >= 8.0"
  ]
}